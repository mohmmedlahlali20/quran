 الشام ومصر والري. فمكث\n          قرابة الخمسة عشرة عامًا في طلب الحديث،\n        </h3>\n        <div className=\"all_categories\">\n          {muslimHadithCategories.map((item) => (\n            <div\n              className=\"category\"\n              key={item.id}\n              onClick={()=> handleHadithCategory(item.nameEn)}\n             >\n              {item.nameAr}\n            </div>\n          ))}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Muslim;\n// \"Sahih Muslim\""],"mappings":";;AAAA,OAAO,eAAe;AACtB,SAASA,cAAc,QAAQ,2BAA2B;AAC1D,SAASC,sBAAsB,QAAQ,kCAAkC;AACzE,SAASC,WAAW,QAAQ,kBAAkB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE/C,MAAMC,MAAM,GAAGA,CAAA,KAAM;EAAAC,EAAA;EACnB,MAAM;IAAEC;EAAM,CAAC,GAAGP,cAAc,CAAC,CAAC;EAClC,MAAMQ,QAAQ,GAAGN,WAAW,CAAC,CAAC;EAE9B,MAAMO,oBAAoB,GAAIC,YAAY,IAAK;IAC7C,MAAMC,UAAU,GAAG,cAAc;IACjC,MAAOC,QAAQ,GAAGF,YAAY;IAC9BG,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEF,QAAQ,CAAC;IAC1CC,YAAY,CAACC,OAAO,CAAC,YAAY,EAAEH,UAAU,CAAC;IAC9CH,QAAQ,CAAC,SAAS,CAAC;EACrB,CAAC;EAGD,oBACEJ,OAAA;IAAKW,SAAS,EAAG,UAASR,KAAM,EAAE;IAAAS,QAAA,eAChCZ,OAAA;MAAKW,SAAS,EAAC,WAAW;MAAAC,QAAA,gBACtBZ,OAAA;QAAAY,QAAA,EAAI;MAEJ;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eACPhB,OAAA;QAAAY,QAAA,EAAI;MAUJ;QAAAC,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eACLhB,OAAA;QAAKW,SAAS,EAAC,gBAAgB;QAAAC,QAAA,EAC5Bf,sBAAsB,CAACoB,GAAG,CAAEC,IAAI,iBAC/BlB,OAAA;UACEW,SAAS,EAAC,UAAU;UAEpBQ,OAAO,EAAEA,CAAA,KAAKd,oBAAoB,CAACa,IAAI,CAACE,MAAM,CAAE;UAAAR,QAAA,EAE/CM,IAAI,CAACG;QAAM,GAHPH,IAAI,CAACI,EAAE;UAAAT,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAIT,CACN;MAAC;QAAAH,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OACC,CAAC;IAAA;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACH;EAAC;IAAAH,QAAA,EAAAC,YAAA;IAAAC,UAAA;IAAAC,YAAA;EAAA,OACH,CAAC;AAEV,CAAC;AAACd,EAAA,CA5CID,MAAM;EAAA,QACQL,cAAc,EACfE,WAAW;AAAA;AAAAyB,EAAA,GAFxBtB,MAAM;AA8CZ,eAAeA,MAAM;AACrB;AAAA,IAAAsB,EAAA;AAAAC,YAAA,CAAAD,EAAA","ignoreList":[]},"metadata":{},"sourceType":"module","extern